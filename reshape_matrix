import tensorflow as tf


tensor1 = tf.ones([1,2,3]) #it will create a martix of 1 and the shape of the matrix is [1,2,3]

print(tensor1)

tensor2 = tf.reshape(tensor1,[2,3,1]) # reshape the matrix 

print(tensor2)

tensor3 =tf.reshape(tensor2,[3,-1]) # -1 tells the tensor to calculate the size of the dimension in that place

print(tensor3)
